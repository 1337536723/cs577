            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/settings" android:name="com.platypus.dicer.SettingsActivity" android:theme="@style/Theme.AppCompat.Light.DarkActionBar"/>
        <activity android:label="@string/pref_about" android:name="com.platypus.dicer.AboutActivity" android:theme="@style/Theme.Base.AppCompat.Dialog.Light.FixedSize">
            <intent-filter>
                <action android:name=".AboutActivity"/>
            </intent-filter>
        </activity>
    </application>
</manifest>
